#!/bin/bash -x

for filename in 'feb24 (HUMAN ORIGINS)' 'feb29 (HOMO ERECTUS)' 'mar2 (NEANDERTHALS)' 'mar7 (AMH Migration etc...)' 'mar9 (ORIGINS AND DEVELOPMENT OF FOOD PRODUCTION)' 'mar14 (ANCIENT NORTH AMERICA)' 'mar16 (Origins and Development of Complex Society)' 'mar21 (Complex Society in SW Asia and Egypt)' 'mar23 (Complex Society in South and East Asia)' 'mar28 (Complex Society in Mesoamerica)' 'mar30 (Complex Society in South America)'; do
    echo -e "--------------------------------------------------------\n\n\n"
    echo -e "================      $filename      ===================\n\n\n"
    cat "$filename"
    echo -e "\n\n\n\n\n"
done > FinalNotes.txt
